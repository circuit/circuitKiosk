{
    "help": {
        "description": "Displays the available commands",
        "command": "showHelp"
    },
    "start": {
        "description": "Joins or initiates a conference call on demand",
        "command": "start"
    },
    "stop": {
        "description": "Quits the conference call",
        "command": "stop"
    },
    "version": {
        "description": "Displays the device's software version",
        "command": "version"
    },
    "logs": {
        "description": "Retrieves and attaches the logfile to the conversation",
        "command": "getLogs"
    },
    "open": {
        "description": "Unlock door",
        "command": "openDoor"
    },
    "temp": {
        "description": "Get Temperature and humidity",
        "command": "getTemp",
        "test": true
    }
}